.c-input-box{
  box-sizing: border-box;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
  height: 100%;
  padding: 0 10rpx;
}
.c-input-box input{
  display: inline-block;
}
.c-input-box .c-clear-btn-box{
  height: 35rpx;
}
.c-input-box .c-clear-btn{
  width: 35rpx;
  height: 35rpx;
}