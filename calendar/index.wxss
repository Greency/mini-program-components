.m-left-10{
  margin-left: 20rpx;
}
.m-right-10{
  margin-right: 20rpx;
}

view.icon-arrows{
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
view.icon-arrows-right{
  background-image: url(data:image/png;base64,);
}
view.icon-arrows-left{
  background-image: url(data:image/png;base64,);
}
view.icon-arrows-double-right{
  background-image: url(data:image/png;base64,);
}
view.icon-arrows-double-left{
  background-image: url(data:image/png;base64,);
}
.day-prev, .day-next{
  color: #666666;
}
.day-active{
  background-color: #FFE600;
}
.day-focus{
  background-color: white;
  border: 1px solid #FFE600;
}

.calendar{
  box-sizing: border-box;
  width: 100%;
  padding: 30rpx 30rpx;
}

.header{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  font-size: 14px;
}

.week-box{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  font-size: 14px;
  margin-top: 20rpx;
}
.week-box .week-item{
  width: 62rpx;
  text-align: center;
}

.day-box{
  margin-top: 20rpx;
}
.day-box .day-item-row{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.day-box .day-item-row .day-item{
  box-sizing: border-box;
  width: 62rpx;
  height: 62rpx;
  line-height: 62rpx;
  text-align: center;
  border-radius: 50%;
}